<!DOCTYPE html>
<html lang="en">
<head>
  <%- include("./partials/head.ejs") %>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
  <%- include("./partials/nav.ejs") %>
    <div class="container">
        <form action="/user/signup" method="post">
            <%- include("./partials/head.ejs") %>
            <div class="mb-4">
                <label for="fullName" class="form-label mt-4">Full Name</label>
                <input type="text" class="form-control "  name="fullName" id="fullName" aria-describedby="fullName">
                <!-- <div id="fullNamehelp" class="form-text">We'll never share your email with anyone else.</div> -->
              </div>
              <div class="mb-4">
            <div class="mb-4">
              <label for="exampleInputEmail1" class="form-label">Email address</label>
              <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp">
              <!-- <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div> -->
            </div>
            <div class="mb-4">
              <label for="exampleInputPassword1" class="form-label">Password</label>
              <input type="password" class="form-control" name="password" id="exampleInputPassword1">
            </div>
            
            <button type="submit" class="btn btn-primary">Submit</button>
          </form>
    </div> 
    <%- include("./partials/script.ejs") %>
</body>
</html>